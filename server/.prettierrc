{
	"singleQuote": false,
	"jsxBracketSameLine": false,
	"trailingComma": "none",
	"printWidth": 120,
	"useTabs": true,
	"semi": true,
	"tabWidth": 4,
	"bracketSpacing": true,
	"endOfLine": "auto",

	"importOrder": [
		"^react",
		"^@?\\w",
		"^(@|components)(/.*|$)",
		"^\\u0000",
		"^\\.\\.(?!/?$)",
		"^\\.\\./?$",
		"^\\./(?=.*/)(?!/?$)",
		"^\\.(?!/?$)",
		"^\\./?$",
		"^.+\\.?(css)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
